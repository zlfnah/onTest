/* Create three equal columns that floats next to each other */
.a_column {
  float: left;
  width: 100%;
  padding: 10px;
  background-color: #ccc;
  height: 250px;
}

.a_column a {
  float: none;
  color: black;
  padding: 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}


.a_column a:hover {
  background-color: #ddd;
}


/* Clear floats after the columns */
.a_row:after {
  content: "";
  display: table;
  clear: both;
}


