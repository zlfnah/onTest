.dropdown-content {
  display: none;
  position: absolute;
  background-color: white;
  width: 100%;
  border:1px;
  left: 0;
  
  z-index: 1;
}

.dropdown-content .header {
  background: red;
  padding: 16px;
  color: white;
}


